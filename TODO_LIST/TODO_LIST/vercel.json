{
  "version": 1,
  "builds": [
    {
      "src": "TODO_LIST/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    },
    {
      "src": "buil_files.sh",
      "use": "@vercel/static-build",
      "config": {"distDir": "staticfiles_build"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TODO_LIST/wsgi.py"
    }
  ]
}
